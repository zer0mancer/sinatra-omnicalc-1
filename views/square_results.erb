<h1>Square Results</h1>

<%#= params %>
<dl>
  <dt>
    Number
  </dt>
  <dd>
    <%= params.fetch('input_number') %>
  </dd>

  <dt>
    Square
  </dt>
  <dd>
   <%= params.fetch('input_number').to_f * params.fetch('input_number').to_f %>
  </dd>
</dl>

<a href="/square/new">
  Calculate another square
</a>
